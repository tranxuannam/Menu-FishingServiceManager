<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fishing.namtran.fishingmanagerservice">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ManagerCustomerActivity" />
        <activity
            android:name="com.fishing.namtran.fishingmanagerservice.SimpleTable"
            android:label="@string/simple_adapter" >
        </activity>
        <activity
            android:name="com.fishing.namtran.fishingmanagerservice.StyleTable"
            android:label="@string/style_adapter" >
        </activity>
        <activity
            android:name="com.fishing.namtran.fishingmanagerservice.FamilyTable"
            android:label="@string/family_adapter" >
        </activity>
    </application>

</manifest>